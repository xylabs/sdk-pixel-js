  "version": "1.0.17",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "@xyo-network/sdk-xyo-js": "^2.1.17",
    "async-mutex": "^0.3.1",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mobile-detect": "^1.4.5",
    "randombytes": "^2.1.0",
    "tslib": "^2.2.0"
  },
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn compile && yarn copy-src",
    "copy-src": "mkdir -p dist && cp package.json dist && cp README.md dist && cp LICENSE dist && cp -R src dist",
    "compile": "rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish ./dist --access public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx,.json ./src --fix",
    "test": "jest",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./dist && rm -r -f ./build",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/md5": "^2.3.0",
    "@types/node": "^16",
    "@types/randombytes": "^2.0.0",
    "@xylabs/eslint-config": "^2.0.3",
    "depcheck": "^1.4.0",
    "jest": "^27.0.4",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-default": "^3.0.0",
    "rollup": "^2.50.6",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "ts-prune": "^0.9.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2",
    "vue": "^3.1.2"
  }
}